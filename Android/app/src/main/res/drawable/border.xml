<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="5dp"
                android:color="#ffb700"/>
            <corners android:radius="10dip"/>
            <solid android:color="#F1BC20"/>
        </shape>
    </item>
</layer-list>

